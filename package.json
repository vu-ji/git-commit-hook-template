{
  "name": "git-commit-jook-template",
  "version": "1.0.0",
  "description": "\"# git-commit-hook-template\"",
  "main": "index.js",
  "scripts": {
    "init-project": "npm install && npm install -g commitizen git-cz",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "git-cz": "^4.7.0",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "git": "https://github.com/vu-ji/git-commit-hook-template",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
